<div class="container">

<form method="GET" action="">

<table class="table">

<tbody>

	<% for User in @users %>
<tr>
	<td rowspan="2">
		<img class="thumb" src="" width = "150px">
	</td>
	<td><a type="text" name="Id" href="/user?Id=<%= User.id %>"><%= User.username %></a></td>
</tr>
<tr>
	<td colspan="4"> <%= User.bio %> </td></tr>
</tr>
<% end %>

</tbody>
</table>


</form>
</div>

